<article id="post-<%= title%>" class="article article-type-post  article-index" itemscope="" itemprop="blogPost">
    <div class="article-inner">
        <header class="article-header">
            <h1 itemprop="name">
                <a class="article-title" href="/article/<%= title%>"><%= title%></a>
            </h1>
            <a href="/article/<%= title%>" class="archive-article-date">
                <time datetime="2018-03-18T12:38:49.000Z" itemprop="datePublished">
                    <i class="icon-calendar icon"></i><%= date%></time>
            </a>
        </header>
        <div class="article-entry" itemprop="articleBody">
            <%- html%>
        </div>
        <% if(tags.length>0) {%>
        <div class="article-info article-info-index"> 
            <div class="article-tag tagcloud">
                <i class="icon-price-tags icon"></i>
                <ul class="article-tag-list">
                    <% for(var i=0;i<tags.length;i++){ %>
                    <li class="article-tag-list-item">
                        <a href="/tag/<%= tags[i]%>" class="js-tag article-tag-list-link color3"><%= tags[i]%></a>
                    </li>
                    <% }%>
                </ul>
            </div>
            <div class="clearfix"></div>
        </div>
        <% }%>
    </div>
</article>